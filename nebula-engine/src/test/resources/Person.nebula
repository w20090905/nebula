type Person {
	!Name;
	Birthday;
	/*Age = Today.year - BirthdayDay.year;*/
	Height;
	Age;
	Sex;
	Detail{
		!Name;
		!Age;
	};
	
	?Roles1[]				Text;	
	Roles2[1..]				Text;
	Roles3[..5]				Text;	
	Roles4[1..5]			Text;	
	
	Education[..5]{
		DateFrom Date;
		DateTo Date;
		School Text;
	};
};