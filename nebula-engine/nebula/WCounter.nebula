@Remarks("Individual Count hit")
@Refby("0")
tx WCounter {
	@Remarks("Individual Count hit")
	!WCounterId ID;
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;
	@Remarks("Organizational entity within client")
	AdOrgId ID;
	@Remarks("Page URL")
	Pageurl URL;
	@Remarks("Referring web address")
	?Referrer Description;
	@Remarks("Remote host Info")
	?RemoteHost Host;
	@Remarks("Remote Address")
	?RemoteAddr Name;
	@Remarks("Browser Used")
	?Useragent Note;
	@Remarks("Language accepted based on browser information")
	?AcceptLanguage Description;
	@Remarks("The document has been processed")
	Processed YesNo;/* 'N'  */
	@Remarks("Web Counter Count Management")
	?WCountercount;
	@Remarks("User within the system - Internal or Business Partner Contact")
	?AdUser;
	@Remarks("Electronic Mail Address")
	?Email;
};

