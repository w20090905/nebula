@Remarks("Media Server list to which content should get transfered")
@Refby("1")
type CmMediaServer {
	!CmMediaServerId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	CmWebproject;
	IsPassive YesNo;/* 'Y'  */
	?Url URL;
	?IpAddress String;
	?Username Name;
	?Password String;
	?Folder String;
};

