@Remarks("ASP Form")
@Refby("0")
type AspForm {
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;/* NULL   */
	@Remarks("Special Form")
	!AdForm;
	@Remarks("Organizational entity within client")
	AdOrgId ID;/* NULL   */
	@Remarks("ASP Level")
	!AspLevel;
	@Remarks("ASP Status")
	AspStatus Attr;/* 'U'  */
};

