@Remarks("System Message")
@Refby("0")
tx AdMessageTrl {
	@Remarks("System Message")
	!AdMessage;
	@Remarks("Language for this entity")
	!AdLanguage;
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;
	@Remarks("Organizational entity within client")
	AdOrgId ID;
	@Remarks("Textual Informational, Menu or Error Message")
	Msgtext Note;
	@Remarks("Additional tip or help for this message")
	?Msgtip Note;
	@Remarks("This column is translated")
	IsTranslated YesNo;/* 'N'  */
};

