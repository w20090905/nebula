@Remarks("ASP Workflow")
@Refby("0")
type AspWorkflow {
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;/* NULL   */
	@Remarks("Organizational entity within client")
	AdOrgId ID;/* NULL   */
	@Remarks("Workflow or combination of tasks")
	!AdWorkflow;
	@Remarks("ASP Level")
	!AspLevel;
	@Remarks("ASP Status")
	AspStatus Attr;/* 'U'  */
};

