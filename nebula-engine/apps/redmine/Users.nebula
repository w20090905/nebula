@Remarks("")
@Refby("0")
type Users {
	!ID;
	*Login String;
	HashedPassword Password;
	FirstName;
	LastName;
	Mail EMail;
	Admin YesNo;/* 0 */
	Status;/* 1 */
	?LastLoginOn Datetime;
	?Language String;
	?AuthSource AuthSources;
	?CreatedOn Datetime;
	?UpdatedOn Datetime;
	?Type Attr;
	?IdentityURL URL;
	MailNotification String;
	?Salt String;
};

