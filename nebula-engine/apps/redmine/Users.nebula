@Remarks("")
@Refby("0")
type Users {
	!Id ID;
	Login String;
	HashedPassword Password;
	Firstname FirstName;
	Lastname LastName;
	Mail EMail;
	Admin YesNo;/* 0 */
	Status;/* 1 */
	?LastLoginOn Datetime;
	?Language String;
	?AuthSource AuthSources;
	?CreatedOn Datetime;
	?UpdatedOn Datetime;
	?Type Attr;
	?IdentityUrl Url;
	MailNotification String;
	?Salt String;
};

