@Remarks("User defined Field")
@Table("AD_USERDEF_FIELD")
type AdUserDefField {
	!AdUserDefFieldId ID;
	AdClientId ID;
	AdOrgId ID;
	AdUserDefTab;
	AdField;
	*Name;
	?Description;
	?Help;
	IsDisplayed YesNo;/* 'Y'  */
	IsReadonly YesNo;/* 'N'  */
	IsSameLine YesNo;/* 'N'  */
	IsUpdateable YesNo;/* 'Y'  */
	DisplayLength Length;/* 0  */
	DisplayLogic String;
	DefaultValue String;
	SortNo SeqNo;/* 0  */
	SeqNo;/* 0  */
};

