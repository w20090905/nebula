@Remarks("Identifies a Menu")
@Table("AD_MENU")
type AdMenu {
	!AdMenuId ID;
	AdClient;
	AdOrg;
	*Name;
	?Description;
	IsSummary YesNo;/* 'N'  */
	IsSoTrx YesNo;/* 'Y'  */
	IsReadonly YesNo;/* 'N'  */
	?Action Attr;
	?AdWindow;
	?AdWorkflow;
	?AdTask;
	?AdProcess;
	?AdForm;
	?AdWorkbench;
	EntityType AdEntityType;/* 'D'  */
};

