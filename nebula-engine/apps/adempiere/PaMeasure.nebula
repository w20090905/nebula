@Remarks("Concrete Performance Measurement")
@Table("PA_MEASURE")
type PaMeasure {
	!PaMeasureId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	MeasureType Attr;
	?ManualActual Number;
	?ManualNote Note;
	?CalculationClass String;
	?PaMeasureCalc;
	?PaBenchmark;
	?PaRatio;
	?PaHierarchy;
	MeasureDataType Attr;
	?RRequestType;
	?CProjectType;
};

