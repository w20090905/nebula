@Remarks("Phase of a Project")
@Refby("8")
type CProjectPhase {
	@Remarks("Financial Project")
	CProject;
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;
	@Remarks("Organizational entity within client")
	AdOrgId ID;
	@Remarks("Optional short description of the record")
	?Description;
	@Remarks("First effective day (inclusive)")
	?StartDate Date;
	@Remarks("Last effective date (inclusive)")
	?EndDate Date;
	@Remarks("It is complete")
	IsComplete YesNo;/* 'N'  	  */
	@Remarks("Product, Service, Item")
	?MProduct;
	@Remarks("Actual Price")
	?PriceActual Number;/* 0  	  */
	@Remarks("Generate Order")
	?GenerateOrder Attr;
	@Remarks("Order")
	?COrder;
	@Remarks("Standard Phase of the Project Type")
	?CPhase;
	@Remarks("Phase of a Project")
	!CProjectPhaseId ID;
	@Remarks("Comment or Hint")
	?Help;
	@Remarks("Alphanumeric identifier of the entity")
	*Name;
	@Remarks("Quantity")
	?Qty Quantity;/* 0  */
	@Remarks("Method of ordering records; lowest number comes first")
	SeqNo;
	@Remarks("The (legal) commitment amount")
	CommittedAmt Amount;/* 0  */
	@Remarks("The commitment amount/quantity is the chargeable ceiling")
	IsCommitCeiling YesNo;/* 'N'  */
	@Remarks("Invoice Rule for the project")
	?ProjInvoiceRule Attr;
	@Remarks("Planned amount for this project")
	PlannedAmt Amount;/* 0  */
};

