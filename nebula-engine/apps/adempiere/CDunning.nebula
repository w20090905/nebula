@Legacy("adempiere")
@Remarks("Dunning Rules for overdue invoices")
@Refby("3")
@Table("C_DUNNING")
type CDunning {
@Column("C_DUNNING_ID") 	!CDunningId ID;
@Column("AD_CLIENT_ID") 	AdClientId ID;
@Column("AD_ORG_ID") 	AdOrgId ID;
	*Name;
	?Description;
	SendDunningLetter YesNo;/* 'Y'  */
	IsDefault YesNo;/* 'N'  */
	CreateLevelsSequentially YesNo;/* 'N'  */
};

