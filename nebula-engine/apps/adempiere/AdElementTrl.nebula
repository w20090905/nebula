@Remarks("System Element enables the central maintenance of column description and help.")
@Table("AD_ELEMENT_TRL")
type AdElementTrl {
	!AdElement;
	!AdLanguage;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	PrintName Name;
	?Description;
	?Help;
	?PoName Name;
	?PoPrintName Name;
	?PoDescription Description;
	?PoHelp Help;
	IsTranslated YesNo;/* 'N'  */
};

