@Legacy("adempiere")
@Remarks("Workflow Node (activity), step or process")
@Refby("8")
@Table("AD_WF_NODE")
type AdWfNode {
@Column("AD_WF_NODE_ID") 	!AdWfNodeId ID;
@Column("AD_CLIENT_ID") 	AdClient;
@Column("AD_ORG_ID") 	AdOrg;
	*Name;
	?Description;
	?Help;
@Column("AD_WORKFLOW_ID") 	AdWorkflow;
	IsCentrallyMaintained YesNo;/* 'Y'  */
	Action Attr;
@Column("AD_WINDOW_ID") 	?AdWindow;
@Column("WORKFLOW_ID") 	?Workflow AdWorkflow;
@Column("AD_TASK_ID") 	?AdTask;
@Column("AD_PROCESS_ID") 	?AdProcess;
@Column("AD_FORM_ID") 	?AdForm;
	EntityType AdEntityType;/* 'D'  */
	XPosition Position;/* 0 */
	YPosition Position;/* 0 */
@Column("AD_WF_BLOCK_ID") 	?AdWfBlock;
	?SubFlowExecution String;
	?StartMode Attr;
	?FinishMode Attr;
	Limit Number;/* 0 */
	?Priority;
	Duration Number;/* 0 */
	Cost Amount;/* 0 */
	?WorkingTime Count;
	WaitingTime Count;/* 0 */
@Column("AD_WF_RESPONSIBLE_ID") 	?AdWfResponsible;
@Column("AD_IMAGE_ID") 	?AdImage;
	JoinElement String;/* NULL */
	SplitElement String;/* NULL */
	?WaitTime Count;
@Column("AD_COLUMN_ID") 	?AdColumn;
	?AttributeName Name;
	?AttributeValue String;
	?DocAction Attr;
	Value String;
	?DynPriorityUnit Attr;
	?DynPriorityChange Number;
	?EmailRecipient Email;
	?Email;
@Column("R_MAILTEXT_ID") 	?RMailText;
};

