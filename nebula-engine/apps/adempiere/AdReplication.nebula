@Remarks("Data Replication Target")
@Table("AD_REPLICATION")
type AdReplication {
	!AdReplicationId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	HostAddress Host;
	HostPort Port;
	AdReplicationStrategy;
	IsRmiOverHttp YesNo;/* 'Y' */
	?Processing YesNo;
	?IdRangeStart Number;
	?IdRangeEnd Number;
	?RemoteClient AdClient;
	?RemoteOrg AdOrg;
	?Prefix String;
	?Suffix String;
	?DateLastRun Date;
};

