@Remarks("User Asset Registration")
@Table("A_REGISTRATION")
type ARegistration {
	!ARegistrationId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	?AAsset;
	?MProduct;
	?CBpartner;
	?AdUser;
	IsRegistered YesNo;/* 'N'  */
	IsInProduction YesNo;/* 'N'  */
	IsAllowPublish YesNo;/* 'Y'  */
	?RemoteHost Host;
	?RemoteAddr Message;
	?Processing YesNo;
	?AssetServiceDate Date;
	?Note;
};

