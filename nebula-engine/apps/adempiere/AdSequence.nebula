@Remarks("Document Sequence")
@Refby("6")
type AdSequence {
	@Remarks("Document Sequence")
	!AdSequenceId ID;
	@Remarks("Client/Tenant for this installation.")
	AdClient;
	@Remarks("Organizational entity within client")
	AdOrg;
	@Remarks("Alphanumeric identifier of the entity")
	*Name;
	@Remarks("Optional short description of the record")
	?Description;
	@Remarks("Format of the value; Can contain fixed format elements, Variables: '_lLoOaAcCa09'")
	?Vformat Description;
	@Remarks("Automatically assign the next number")
	IsAutoSequence YesNo;/* 'Y'  */
	@Remarks("The number to increment the last document number by")
	IncrementNo SeqNo;
	@Remarks("Starting number/position")
	StartNo SeqNo;
	@Remarks("The next number to be used")
	CurrentNext Number;
	@Remarks("Next sequence for system use")
	CurrentNextSys Number;
	@Remarks("Activate Audit Trail of what numbers are generated")
	?IsAudited YesNo;/* 'N'                        */
	@Remarks("The document number  will be used as the record key")
	?IsTableId YesNo;/* 'N'                        */
	@Remarks("Prefix before the sequence number")
	?Prefix Description;/* NULL */
	@Remarks("Suffix after the number")
	?Suffix Description;/* NULL */
	@Remarks("Restart the sequence with Start on every 1/1")
	?StartNewYear YesNo;/* 'N'                        */
	@Remarks("Fully qualified date column")
	?DateColumn String;
	@Remarks("Java Decimal Pattern")
	?DecimalPattern null;
};

