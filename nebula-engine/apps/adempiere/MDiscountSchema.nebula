@Remarks("Schema to calculate the trade discount percentage")
@Refby("7")
type MDiscountSchema {
	!MDiscountSchemaId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	ValidFrom Date;
	DiscountType Attr;
	?Script String;
	?FlatDiscount Discount;
	IsQuantityBased YesNo;/* 'Y'  */
	?CumulativeLevel Attr;
	?Processing YesNo;
	IsBpartnerFlatDiscount YesNo;/* 'N'  */
};

