@Remarks("Schema to calculate the trade discount percentage")
@Refby("7")
type MDiscountSchema {
	@Remarks("Schema to calculate the trade discount percentage")
	!MDiscountSchemaId ID;
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;
	@Remarks("Organizational entity within client")
	AdOrgId ID;
	@Remarks("Alphanumeric identifier of the entity")
	*Name;
	@Remarks("Optional short description of the record")
	?Description;
	@Remarks("Valid from including this date (first day)")
	ValidFrom Date;
	@Remarks("Type of trade discount calculation")
	DiscountType Attr;
	@Remarks("Dynamic Java Language Script to calculate result")
	?Script Note;
	@Remarks("Flat discount percentage")
	?FlatDiscount Count;
	@Remarks("Trade discount break level based on Quantity (not value)")
	IsQuantityBased YesNo;/* 'Y'  */
	@Remarks("Level for accumulative calculations")
	?CumulativeLevel Attr;
	@Remarks("Process Now")
	?Processing YesNo;
	@Remarks("Use flat discount defined on Business Partner Level")
	IsBpartnerFlatDiscount YesNo;/* 'N'  */
};

