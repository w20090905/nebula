@Legacy("adempiere")
@Remarks("Schema to calculate the trade discount percentage")
@Refby("7")
@Table("M_DISCOUNTSCHEMA")
type MDiscountSchema {
@Column("M_DISCOUNTSCHEMA_ID") 	!MDiscountSchemaId ID;
@Column("AD_CLIENT_ID") 	AdClientId ID;
@Column("AD_ORG_ID") 	AdOrgId ID;
	*Name;
	?Description;
	ValidFrom Date;
	DiscountType Attr;
	?Script String;
	?FlatDiscount Percent;
	IsQuantityBased YesNo;/* 'Y'  */
	?CumulativeLevel Attr;
	?Processing YesNo;
	IsBpartnerFlatDiscount YesNo;/* 'N'  */
};

