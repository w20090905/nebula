@Remarks("Unique line in an Inventory document")
@Table("M_INVENTORYLINE")
tx MInventoryLine {
	!MInventoryLineId ID;
	AdClientId ID;
	AdOrgId ID;
	MInventory;
	MLocator;
	MProduct;
	?Line Number;
	QtyBook Number;/* 0  */
	QtyCount Count;/* 0  */
	?Description;
	?MAttributeSetInstance;/* 0 */
	?CCharge;
	InventoryType Attr;/* 'D'  */
	Processed YesNo;/* 'N'  */
	?QtyInternalUse Number;
};

