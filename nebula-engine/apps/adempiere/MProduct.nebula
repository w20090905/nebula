@Remarks("Product, Service, Item")
@Refby("89")
type MProduct {
	@Remarks("Product, Service, Item")
	!MProductId ID;
	@Remarks("Client/Tenant for this installation.")
	AdClient;
	@Remarks("Organizational entity within client")
	AdOrg;
	@Remarks("Search key for the record in the format required - must be unique")
	Value String;
	@Remarks("Alphanumeric identifier of the entity")
	*Name;
	@Remarks("Optional short description of the record")
	?Description;
	@Remarks("Additional information for a Document")
	?DocumentNote Note;
	@Remarks("Comment or Hint")
	?Help;
	@Remarks("Bar Code (Universal Product Code or its superset European Article Number)")
	?UPC;
	@Remarks("Stock Keeping Unit")
	?SKU;
	@Remarks("Unit of Measure")
	CUom;
	@Remarks("Sales Representative or Company Agent")
	?SalesRep AdUser;
	@Remarks("This is a summary entity")
	IsSummary YesNo;/* 'N'  */
	@Remarks("Organization stocks this product")
	IsStocked YesNo;/* 'Y'  */
	@Remarks("Organization purchases this product")
	IsPurchased YesNo;/* 'Y'  */
	@Remarks("Organization sells this product")
	IsSold YesNo;/* 'Y'  */
	@Remarks("Bill of Materials")
	IsBom YesNo;/* 'N'  */
	@Remarks("Print detail BOM elements on the invoice")
	IsInvoicePrintDetails YesNo;/* 'N'  */
	@Remarks("Print detail BOM elements on the pick list")
	IsPickListPrintDetails YesNo;/* 'N'  */
	@Remarks("The BOM configuration has been verified")
	IsVerified YesNo;/* 'N'  */
	@Remarks("Method for recording revenue")
	?CRevenueRecognition;
	@Remarks("Category of a Product")
	MProductCategory;
	@Remarks("Classification for grouping")
	?ClassIfication YesNo;
	@Remarks("Volume of a product")
	?Volume;/* 0 */
	@Remarks("Weight of a product")
	?Weight;/* 0 */
	@Remarks("Shelf width required")
	?ShelfWidth Width;
	@Remarks("Shelf height required")
	?ShelfHeight Height;
	@Remarks("Shelf depth required")
	?ShelfDepth Depth;
	@Remarks("Units Per Pallet")
	?UnitsPerPallet Number;
	@Remarks("Tax Category")
	CTaxCategory;
	@Remarks("Resource")
	?SResource;
	@Remarks("This product is no longer available")
	?Discontinued YesNo;/* 'N'                                 */
	@Remarks("Discontinued By")
	?DiscontinuedBy Date;
	@Remarks("Process Now")
	?Processing YesNo;
	@Remarks("Expense report type")
	?SExpenseType;
	@Remarks("Type of product")
	ProductType Attr;/* 'I'  */
	@Remarks("URL of  image")
	?ImageUrl Url;
	@Remarks("URL for the description")
	?DescriptionUrl Description;
	@Remarks("Number of days the product is guaranteed or available")
	?GuaranteeDays Count;
	@Remarks("Text templates for mailings")
	?RMailText;
	@Remarks("Version Number")
	?VersionNo Code;
	@Remarks("Product Attribute Set")
	?MAttributeSet;
	@Remarks("Product Attribute Set Instance")
	?MAttributeSetInstance;/* 0 */
	@Remarks("URL of the Download files")
	?DownloadUrl Url;
	@Remarks("Category of the Freight")
	?MFreightCategory;
	@Remarks("Warehouse Locator")
	?MLocator;
	@Remarks("Minumum number of guarantee days")
	?GuaranteeDaysMin Number;
	@Remarks("If selected, the product is displayed in the inital or any empy search")
	IsWebStoreFeatured YesNo;/* 'N'  */
	@Remarks("This is a Self-Service entry or this entry can be changed via Self-Service")
	IsSelfService YesNo;/* 'Y'  */
	@Remarks("Type of subscription")
	?CSubscriptionType;
	@Remarks("Drop Shipments are sent from the Vendor directly to the Customer")
	IsDropShip YesNo;/* 'N'  */
	@Remarks("Exclude from automatic Delivery")
	IsExcludeAutoDelivery YesNo;/* 'N'  */
	@Remarks("Group1")
	?Group1 null;
	@Remarks("Group2")
	?Group2 null;
};

