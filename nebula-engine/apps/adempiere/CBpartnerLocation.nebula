@Remarks("Identifies the (ship to) address for this Business Partner")
@Table("C_BPARTNER_LOCATION")
type CBpartnerLocation {
	!CBpartnerLocationId ID;
	AdClient;
	AdOrg;
	*Name;
	IsBillTo YesNo;/* 'Y'  */
	IsShipTo YesNo;/* 'Y'  */
	IsPayFrom YesNo;/* 'Y'  */
	IsRemitTo YesNo;/* 'Y'  */
	?Phone;
	?Phone2 Phone;
	?Fax;
	?Isdn;
	?CSalesRegion;
	CBpartner;
	?CLocation;
};

