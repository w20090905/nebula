@Remarks("A web project is the main data container for Containers, URLs, Ads, Media etc.")
@Table("CM_WEBPROJECT")
type CmWebProject {
	!CmWebProjectId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	MetaCopyRight String;
	MetaPublisher String;
	MetaRobotsTag String;
	MetaAuthor String;
	MetaContent String;
	AdTreeCmc AdTree;
	AdTreeCms AdTree;
	AdTreeCmm AdTree;
	AdTreeCmt AdTree;
};

