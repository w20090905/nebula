@Remarks("Media Server list to which content should get transfered")
@Table("CM_MEDIA_SERVER")
type CmMediaServer {
	!CmMediaServerId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	CmWebProject;
	IsPassive YesNo;/* 'Y'  */
	?Url;
	?IpAddress Address;
	?UserName Name;
	?Password String;
	?Folder String;
};

