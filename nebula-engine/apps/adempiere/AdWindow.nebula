@Legacy("adempiere")
@Remarks("Data entry or display window")
@Refby("17")
@Table("AD_WINDOW")
type AdWindow {
@Column("AD_WINDOW_ID") 	!AdWindowId ID;
@Column("AD_CLIENT_ID") 	AdClient;
@Column("AD_ORG_ID") 	AdOrg;
	*Name;
	?Description;
	?Help;
	?WindowType Attr;
	IsSoTrx YesNo;/* 'Y'  */
	EntityType AdEntityType;/* 'D'  */
	?Processing YesNo;
@Column("AD_IMAGE_ID") 	?AdImage;
@Column("AD_COLOR_ID") 	?AdColor;
	IsDefault YesNo;/* 'N' */
	?WinHeight Height;
	?WinWidth Width;
	IsBetaFunctionality YesNo;/* 'N'  */
};

