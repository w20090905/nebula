@Legacy("adempiere")
@Remarks("Workflow or combination of tasks")
@Refby("13")
@Table("AD_WORKFLOW")
type AdWorkflow {
@Column("AD_WORKFLOW_ID") 	!AdWorkflowId ID;
@Column("AD_CLIENT_ID") 	AdClient;
@Column("AD_ORG_ID") 	AdOrg;
	*Name;
	?Description;
	?Help;
	AccessLevel Attr;
@Column("AD_WF_NODE_ID") 	?AdWfNode;
	EntityType AdEntityType;/* 'D'  */
	?DurationUnit Attr;
	Author String;/* 'ComPiere' */
	Version Number;/* 0 */
	?ValidFrom Date;
	?ValidTo Date;
	?Priority;
	?Limit Number;
	Duration Number;/* 0 */
	Cost Amount;/* 0 */
	WorkingTime Count;/* 0 */
	WaitingTime Count;/* 0 */
@Column("AD_WF_RESPONSIBLE_ID") 	?AdWfResponsible;
	PublishStatus Attr;/* NULL */
@Column("AD_WORKFLOWPROCESSOR_ID") 	?AdWorkflowProcessor;
	Value String;
	IsDefault YesNo;/* 'N'  */
@Column("AD_TABLE_ID") 	?AdTable;
	?ValidateWorkflow String;
	WorkflowType Attr;/* 'G'  */
	?DocValueLogic String;
	IsValid YesNo;/* 'N'  */
};

