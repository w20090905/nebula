@Remarks("Template defines how content is displayed")
@Table("CM_TEMPLATE")
type CmTemplate {
	!CmTemplateId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	?CmWebProject;
	Value String;
	IsInclude YesNo;/* 'Y'  */
	IsUseAd YesNo;/* 'Y'  */
	IsNews YesNo;/* 'Y'  */
	?ElementS String;
	?TemplateXst null;
	IsSummary YesNo;/* 'N'  */
	IsValid YesNo;/* 'N'  */
	?Processing YesNo;
};

