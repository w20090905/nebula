@Remarks("Tax identifier")
@Table("C_TAX")
type CTax {
	!CTaxId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?TaxIndicator String;
	IsDocumentLevel YesNo;/* 'Y'  */
	ValidFrom Date;
	IsSummary YesNo;/* 'N'  */
	RequireStaxCertificate YesNo;/* 'N'  */
	Rate Amount;
	?ParentTax CTax;
	?CCountry;
	?CRegion;
	?ToCountry CCountry;
	?ToRegion CRegion;
	CTaxCategory;
	IsDefault YesNo;/* 'N'  */
	IsTaxExempt YesNo;/* 'N'  */
	SoPoType Attr;/* 'B'  */
	IsSalesTax YesNo;/* 'N'  */
};

