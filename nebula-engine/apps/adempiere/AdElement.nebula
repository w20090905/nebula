@Remarks("System Element enables the central maintenance of column description and help.")
@Table("AD_ELEMENT")
type AdElement {
	!AdElementId ID;
	AdClientId ID;
	AdOrgId ID;/* NULL */
	ColumnName Name;
	EntityType AdEntityType;/* 'D'  */
	*Name;/* NULL */
	PrintName Name;
	?Description;
	?Help;
	?PoName Name;
	?PoPrintName Name;
	?PoDescription Description;
	?PoHelp Help;
};

