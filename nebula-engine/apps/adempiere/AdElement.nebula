@Remarks("System Element enables the central maintenance of column description and help.")
@Refby("4")
type AdElement {
	!AdElementId Number;
	AdClientId Number;
	AdOrgId Number;/* NULL */
	ColumnName Name;
	EntityType AdEntityType;/* 'D'  */
	*Name;/* NULL */
	PrintName Name;
	?Description;
	?Help;
	?PoName Name;
	?PoPrintName Name;
	?PoDescription Description;
	?PoHelp Help;
};

