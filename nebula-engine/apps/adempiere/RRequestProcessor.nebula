@Remarks("Request Processor")
@Table("R_REQUESTPROCESSOR")
type RRequestProcessor {
	!RRequestProcessorId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	Frequency;
	FrequencyType Attr;
	?DateLastRun Date;
	?DateNextRun Date;
	?Processing YesNo;
	OverdueAlertDays Count;
	OverdueAsSignDays Count;
	Supervisor AdUser;
	KeepLogDays Count;
	RemindDays Count;/* 7  */
	InactivityAlertDays Count;/* 0  */
	?RRequestType;
};

