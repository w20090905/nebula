@Remarks("Payment identifier")
@Refby("16")
tx CPayment {
	@Remarks("Payment identifier")
	!CPaymentId ID;
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;
	@Remarks("Organizational entity within client")
	AdOrg;
	@Remarks("Document sequence number of the document")
	DocumentNo Code;
	@Remarks("Transaction Date")
	DateTrx Date;
	@Remarks("This is a sales transaction (receipt)")
	IsReceipt YesNo;/* 'Y'  */
	@Remarks("Document type or rules")
	CDocType;
	@Remarks("Type of credit card transaction")
	TrxType Attr;
	@Remarks("Account at the Bank")
	CBankAccount;
	@Remarks("Identifies a Business Partner")
	?CBpartner;
	@Remarks("Invoice Identifier")
	?CInvoice;
	@Remarks("Bank Account of the Business Partner")
	?CBpBankAccount;
	@Remarks("Payment batch for EFT")
	?CPaymentBatch;
	@Remarks("Method of Payment")
	TenderType Attr;
	@Remarks("Credit Card (Visa, MC, AmEx)")
	?CreditCardType Attr;
	@Remarks("Credit Card Number")
	?CreditCardNumber Code;
	@Remarks("Credit Card Verification code on credit card")
	?CreditCardVv String;
	@Remarks("Expiry Month")
	?CreditCardExpMm Number;
	@Remarks("Expiry Year")
	?CreditCardExpYy Number;
	@Remarks("Combination of routing no, account and check no")
	?Micr null;
	@Remarks("Bank Routing Number")
	?RoutingNo Code;
	@Remarks("Account Number")
	?AccountNo Code;
	@Remarks("Check Number")
	?CheckNo Code;
	@Remarks("Name on Credit Card or Account holder")
	?AName Name;
	@Remarks("Street address of the Credit Card or Account holder")
	?AStreet String;
	@Remarks("City or the Credit Card or Account Holder")
	?ACity String;
	@Remarks("State of the Credit Card or Account holder")
	?AState String;
	@Remarks("Zip Code of the Credit Card or Account Holder")
	?AZip String;
	@Remarks("Payment Identification - Driver License")
	?AIdentDl String;
	@Remarks("Payment Identification - Social Security No")
	?AIdentSsn String;
	@Remarks("Email Address")
	?AEmail EMail;
	@Remarks("Voice Authorization Code from credit card company")
	?VoiceAuthCode Note;
	@Remarks("Original Transaction ID")
	?OrigTrxId Code;
	@Remarks("Purchase Order Number")
	?PoNum Code;
	@Remarks("The Currency for this record")
	CCurrency;
	@Remarks("Amount being paid")
	PayAmt Amount;/* 0  */
	@Remarks("Calculated amount of discount")
	?DiscountAmt Amount;/* 0 */
	@Remarks("Amount to write-off")
	?WriteOffAmt Amount;/* 0 */
	@Remarks("Tax Amount for a document")
	?TaxAmt Amount;/* 0 */
