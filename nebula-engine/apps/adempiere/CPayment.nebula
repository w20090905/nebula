	IsOverUnderPayment YesNo;/* 'N'  */
	@Remarks("Over-Payment (unallocated) or Under-Payment (partial payment) Amount")
	?OverUnderAmt Amount;/* 0 */
	@Remarks("Country")
	?ACountry String;
	@Remarks("Financial Project")
	?CProject;
	@Remarks("This is a Self-Service entry or this entry can be changed via Self-Service")
	IsSelfService YesNo;/* 'N' */
	@Remarks("Charge Amount")
	?ChargeAmt Amount;/* 0 */
	@Remarks("Additional document charges")
	?CCharge;
	@Remarks("Charge after Shipment")
	IsDelayedCapture YesNo;/* 'N'  */
