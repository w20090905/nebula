	IsApproved YesNo;/* 'N'  */
	@Remarks("Payment reference")
	?RPnref String;
	@Remarks("Result of transmission")
	?RResult String;
	@Remarks("Response message")
	?RRespmsg Msg;
	@Remarks("Authorization Code returned")
	?RAuthCode Note;
	@Remarks("This address has been verified")
	?RAvsAddr Address;
	@Remarks("The Zip Code has been verified")
	?RAvsZip String;
	@Remarks("Response info")
	?RInfo Info;
	@Remarks("Process Now")
	?Processing YesNo;
	@Remarks("This payment can be processed online")
	?Oprocessing null;
	@Remarks("The current status of the document")
	DocStatus Attr;
	@Remarks("The targeted status of the document")
	DocAction Attr;
	@Remarks("Payment is reconciled with bank statement")
	IsReconciled YesNo;/* 'N'  */
	@Remarks("Indicates if the payment has been allocated")
	IsAllocated YesNo;/* 'N'  */
	@Remarks("Can be accessed online")
	IsOnline YesNo;/* 'N'  */
	@Remarks("The document has been processed")
	Processed YesNo;/* 'N'  */
	@Remarks("Posting status")
	PostEd YesNo;/* 'N'  */
