@Remarks("ASP Tab")
@Refby("0")
type AspTab {
	@Remarks("Client/Tenant for this installation.")
	AdClientId ID;/* NULL   */
	@Remarks("Organizational entity within client")
	AdOrgId ID;/* NULL   */
	@Remarks("Tab within a Window")
	!AdTab;
	@Remarks("Data entry or display window")
	!AdWindow;
	@Remarks("ASP Level")
	!AspLevel;
	@Remarks("ASP Status")
	AspStatus Attr;/* 'U'  */
	@Remarks("AllFields")
	?AllFields YesNo;/* 'Y'  */
	@Remarks("Process Now")
	?Processing YesNo;
};

