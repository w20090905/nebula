@Remarks("An Advertisement is something like a banner")
@Table("CM_AD")
type CmAd {
	!CmAdId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	CmAdCat;
	CmMedia;
	?TargetUrl Url;
	TargetFrame String;
	ActualClick Number;/* 0  */
	MaxClick Number;/* 0  */
	ActualImpRession Number;/* 0  */
	MaxImpRession Number;/* 0  */
	StartImpRession Number;/* 0  */
	StartDate Date;
	?EndDate Date;
	?ContentHtml String;
	IsAdFlag YesNo;/* 'Y'  */
	IsLogged YesNo;/* 'Y'  */
};

