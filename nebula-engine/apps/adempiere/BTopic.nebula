@Remarks("Auction Topic")
@Table("B_TOPIC")
type BTopic {
	!BTopicId ID;
	AdClientId ID;
	AdOrgId ID;
	DocumentNo String;
	*Name;
	?Description;
	?TextMsg Text;
	TopicStatus Attr;
	TopicAction Attr;
	IsPublished YesNo;/* 'Y'  */
	?TextDetails null;
	?Processing YesNo;
	Processed YesNo;/* 'N'  */
	BTopicType;
	BTopicCategory;
	DecisionDate Date;
};

