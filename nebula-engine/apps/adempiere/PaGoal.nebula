@Remarks("Performance Goal")
@Table("PA_GOAL")
type PaGoal {
	!PaGoalId ID;
	AdClientId ID;
	AdOrgId ID;
	SeqNo;/* 0  */
	*Name;
	?Description;
	?Note;
	?AdUser;
	PaColorSchema;
	IsSummary YesNo;/* 'N'  */
	?PaGoalParent PaGoal;
	?PaMeasure;
	?RelativeWeight Weight;
	MeasureTarget Number;/* 0  */
	MeasureScope String;
	?MeasureDisplay String;
	?DateFrom Date;
	?DateTo Date;
	?MeasureActual Number;/* 0 */
	?GoalPerformance Number;
	?DateLastRun Date;
	?AdRole;
};

