@Remarks("Adempiere Alert")
@Table("AD_ALERT")
type AdAlert {
	!AdAlertId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	?Help;
	AlertSubject String;
	AlertMessage String;
	EnforceClientSecurity YesNo;/* 'Y'  */
	EnforceRoleSecurity YesNo;/* 'Y'  */
	?AdAlertProcessor;
	IsValid YesNo;/* 'Y'  */
};

