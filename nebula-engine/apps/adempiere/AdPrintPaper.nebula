@Remarks("Printer paper definition")
@Table("AD_PRINTPAPER")
type AdPrintPaper {
	!AdPrintPaperId ID;
	AdClientId ID;
	AdOrgId ID;
	*Name;
	?Description;
	IsDefault YesNo;/* 'N'  */
	IsLandscape YesNo;/* 'Y'  */
	Code;
	MarginTop Length;/* 36 */
	MarginLeft Length;/* 36  */
	MarginRight Length;/* 36  */
	MarginBottom Length;/* 36  */
	?Processing YesNo;
	?SizeX Length;/* NULL  */
	?SizeY Length;/* NULL  */
	?DimensionUnits String;
};

