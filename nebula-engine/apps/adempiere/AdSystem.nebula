	@Remarks("Information")
	?Info;
	@Remarks("Version of the table definition")
	?Version String;
	@Remarks("Internal Release Number")
	ReleaseNo String;
	@Remarks("Number of Internal Users for Adempiere Support")
	?SupportUnits null;
	@Remarks("Password of any length (case sensitive)")
	?Password;
